package Generic_3;

public class Main {

	public static void main(String[] args) {

		Sayisal s1 = new Sayisal("omer", 1, 74.5);
		Sayisal s3 = new Sayisal("faruk", 2, 86.2);
		Sozel s2 = new Sozel("veli", 864, 87.5);
		Sozel S4 = new Sozel("ali", 5, 45.1);
	
	
	}

	public static <T extends Student> T hesapla(T i,T j) {
		if(i.donem_ortalamasi()> j.donem_ortalamasi())
			return i;
	}
}
